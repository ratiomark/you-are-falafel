    "noEmit": true,
    "module": "CommonJS",
    "target": "es2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "tsup.config.ts",
    "."
  ],
  "exclude": [
    "node_modules"
  ]
}